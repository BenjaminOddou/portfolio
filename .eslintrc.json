{
    "env": {
        "browser": true,
        "es2022": true,
        "node": true
    },
    "extends": [
        "@nuxtjs/eslint-config-typescript",
        "plugin:nuxt/recommended",
        "plugin:prettier/recommended"
    ],
    "ignorePatterns": [
        "tmp/**",
        "types/**"
    ],
    "rules": {
        "import/no-named-as-default": 0,
        "no-console": "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ]
    }
}